<resources>

    <!-- Base application theme. -->
    <style name="chat_module_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/chat_module_colorPrimary</item>
        <item name="colorPrimaryDark">@color/chat_module_colorPrimaryDark</item>
        <item name="colorAccent">@color/chat_module_colorAccent</item>
    </style>

    <style name="chat_module_AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="chat_module_AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="chat_module_AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
